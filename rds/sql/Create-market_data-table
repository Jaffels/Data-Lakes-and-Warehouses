CREATE TABLE market_data (
    date                  DATE,
    sp500                 DOUBLE PRECISION,
    nasdaq                DOUBLE PRECISION,
    bitcoin_price         DOUBLE PRECISION,
    ethereum_price        DOUBLE PRECISION,
    oil                   DOUBLE PRECISION,
    gold                  DOUBLE PRECISION,
    copper                DOUBLE PRECISION,
    usdchf                DOUBLE PRECISION,
    eurusd                DOUBLE PRECISION,
    gbpusd                DOUBLE PRECISION,
    usdjpy                DOUBLE PRECISION,
    policy_rate_us        DOUBLE PRECISION,
    cpi_us                DOUBLE PRECISION,
    gdp_us                DOUBLE PRECISION,
    m2_us                 DOUBLE PRECISION,
    cpi_ea                DOUBLE PRECISION,
    gdp_ea                DOUBLE PRECISION,
    cpi_ch                DOUBLE PRECISION,
    cpi_uk                DOUBLE PRECISION,
    cpi_jp                DOUBLE PRECISION,
    ecb_main_rate         DOUBLE PRECISION,
    policy_rate_uk        DOUBLE PRECISION,
    policy_rate_ch        DOUBLE PRECISION,
    policy_rate_jp        DOUBLE PRECISION,
    cc_jp                 DOUBLE PRECISION,
    cc_uk                 DOUBLE PRECISION,
    cc_us                 DOUBLE PRECISION,
    cc_ch                 DOUBLE PRECISION,
    eu_cc_confidence      DOUBLE PRECISION,
    btc_ret               DOUBLE PRECISION,
    eth_ret               DOUBLE PRECISION,
    sp500_ret             DOUBLE PRECISION,
    nasdaq_ret            DOUBLE PRECISION,
    gold_ret              DOUBLE PRECISION,
    copper_ret            DOUBLE PRECISION,
    oil_ret               DOUBLE PRECISION,
    btc_vol_7d            DOUBLE PRECISION,
    btc_vol_7d_annualized DOUBLE PRECISION,
    eth_vol_7d            DOUBLE PRECISION,
    eth_vol_7d_annualized DOUBLE PRECISION,
    sp500_vol_7d          DOUBLE PRECISION,
    sp500_vol_7d_annualized DOUBLE PRECISION,
    nasdaq_vol_7d         DOUBLE PRECISION,
    nasdaq_vol_7d_annualized DOUBLE PRECISION,
    gold_vol_7d           DOUBLE PRECISION,
    gold_vol_7d_annualized DOUBLE PRECISION,
    copper_vol_7d         DOUBLE PRECISION,
    copper_vol_7d_annualized DOUBLE PRECISION,
    oil_vol_7d            DOUBLE PRECISION,
    oil_vol_7d_annualized DOUBLE PRECISION,
    btc_vol_30d           DOUBLE PRECISION,
    eth_vol_30d           DOUBLE PRECISION,
    sp500_vol_30d         DOUBLE PRECISION,
    gold_vol_30d          DOUBLE PRECISION,
    copper_vol_30d        DOUBLE PRECISION,
    oil_vol_30d           DOUBLE PRECISION,
    corr_btc_sp500_90d    DOUBLE PRECISION,
    btc_norm              DOUBLE PRECISION,
    eth_norm              DOUBLE PRECISION,
    sp500_norm            DOUBLE PRECISION,
    nasdaq_norm           DOUBLE PRECISION,
    gold_norm             DOUBLE PRECISION,
    copper_norm           DOUBLE PRECISION,
    oil_norm              DOUBLE PRECISION,
    policy_rate_us_z      DOUBLE PRECISION,
    ecb_main_rate_z       DOUBLE PRECISION,
    policy_rate_uk_z      DOUBLE PRECISION,
    policy_rate_ch_z      DOUBLE PRECISION,
    policy_rate_jp_z      DOUBLE PRECISION,
    cc_jp_z               DOUBLE PRECISION,
    cc_uk_z               DOUBLE PRECISION,
    cc_us_z               DOUBLE PRECISION,
    cc_ch_z               DOUBLE PRECISION,
    eu_cc_confidence_z    DOUBLE PRECISION
);
