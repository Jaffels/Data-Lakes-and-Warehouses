{
  "family": "stock-list-generator-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "4096",
  "executionRoleArn": "arn:aws:iam::404264700821:role/DataPipelineECSTaskRoleV2",
  "taskRoleArn": "arn:aws:iam::404264700821:role/DataPipelineECSTaskRoleV2",
  "containerDefinitions": [
    {
      "name": "stock-list-generator",
      "image": "404264700821.dkr.ecr.us-east-1.amazonaws.com/stock-list-generator:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/stock-list-generator",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      },
      "environment": [
        {
          "name": "AWS_DEFAULT_REGION",
          "value": "us-east-1"
        }
      ]
    }
  ]
}
